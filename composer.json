{
  "name": "developing4fun/app-skeleton",
  "description": "Basic skeleton for incoming projects. This skeleton provides a basic folder structure and a simple Docker environment.",
  "type": "project",
  "authors": [
    {
      "name":"thefoovault"
    }
  ],
  "autoload": {
    "psr-4": {
      "Shared\\": "src/Shared",
      "LobbyWars\\": "src/LobbyWars",
      "LobbyWarsConsole\\": "apps/LobbyWarsConsole/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "test"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "johnkary/phpunit-speedtrap": "^3.2"
  },
  "require": {
    "symfony/framework-bundle": "^5.2",
    "symfony/messenger": "^5.2",
    "symfony/dotenv": "^5.2",
    "symfony/yaml": "^5.2",
    "symfony/console": "^5.2"
  }
}
